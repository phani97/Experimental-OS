load(7,1);// exception handler
load(8,2);

load(23,17); // loading int 7
load(24,18);

load(25,21);  // loading init
load(26,22);
load(27,23);

[READY_LIST + 0] = 0;

PTBR = 1024;
PTLR = 4;

[PTBR + 0] = 25;
[PTBR + 1] = "01";
[PTBR + 2] = 26;
[PTBR + 3] = "01";
[PTBR + 4] = 27;
[PTBR + 5] = "01";
[PTBR + 6] = 28;
[PTBR + 7] = "01";

[READY_LIST + 1] = 2;

SP = 3 * 512;

[28 * 512] = 0;

alias i S0;
i=0; //setting per process open file table with -1

while(i<8) do
	[READY_LIST+15+i*2]=-1;
	[READY_LIST+15+i*2+1]= 0;
	i=i+1;
endwhile;

i=0; //setting system wide open file table with -1

while(i<64) do
	[FILE_TABLE + i*2 + 0] = -1;
	[FILE_TABLE + i*2 + 1] = 0;
	i=i+1;
endwhile;

i=0;
while(i<64) do //setting MEM_LIST
	if(i<=28) then 
		[MEM_LIST+i]=1;
	else 
		[MEM_LIST+i]=0;
	endif;
	i=i+1;
endwhile; 

i=1;
while(i<32) do //setting MEM_LIST
	[READY_LIST+i*32+1]=0;
	i=i+1;
endwhile; 

load(13,7); // loading INT 2
load(14,8);

load(11,5); // loading INT 1
load(12,6);

load(15,9); // loading INT 3
load(16,10);

load(17,11); // loading INT 4
load(18,12);

load(19,13); //loading int5
load(20,14);

load(9,3); //loading timer
load(10,4);

load(5,19) ; // loading FAT

load(6,20); // loading diskfree list

ireturn;
